    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint src/**/*.js && prettier --l \"src/**/*.js\"",
    "lint:fix": "eslint src/**/*.js --fix",
    "prepare": "cd .. && husky install ./frontend/.husky && cp ./frontend/.husky/_/husky.sh ./frontend/.husky/_/husky2.sh && echo 'cd ./frontend && npx lint-staged' > ./frontend/.husky/_/husky.sh && cat ./frontend/.husky/_/husky2.sh >> ./frontend/.husky/_/husky.sh && rm ./frontend/.husky/_/husky2.sh ",
    "prettify-all": "pretty-quick ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/preset-env": "^7.15.6",
